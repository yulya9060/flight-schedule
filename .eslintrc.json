{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "mocha": true
    },
    "rules": {
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4],
        "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
        "no-underscore-dangle": 0,
        "jsx-a11y/label-has-for": 0,
        "react/jsx-boolean-value": 0,
        "no-console": 0,
        "jsx-a11y/no-autofocus": 0
    }
}
